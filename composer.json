{
    "name": "kylekatarnls/multi-tester",
    "description": "Test your package change inside other packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KyleK",
            "email": "kylekatarnls@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "composer/metadata-minifier": "^1.0",
        "composer/semver": "^1.0 || ^2.0 || ^3.0",
        "symfony/yaml": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.46 || ^9.6.27"
    },
    "autoload": {
        "psr-4": {
            "MultiTester\\": "./src/MultiTester"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MultiTester\\Tests\\": "./tests"
        }
    },
    "bin": ["bin/multi-tester"],
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
